{
  "name": "ai-career-compass-jk",
  "version": "2.0.0",
  "description": "AI-powered career guidance platform for Jammu & Kashmir students",
  "main": "backend/server-enhanced.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "vercel-build": "cd frontend && npm install && npm run build",
    "deploy": "npm run build && vercel --prod",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "test": "echo \"Tests coming soon...\" && exit 0"
  },
  "keywords": [
    "career-guidance",
    "jammu-kashmir",
    "education",
    "ai",
    "react",
    "nodejs",
    "vite",
    "tailwindcss",
    "vercel"
  ],
  "author": "Wajid Daud Tamboli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wajiddaudtamboli/AI-Career-Compass-J-K.git"
  },
  "homepage": "https://github.com/wajiddaudtamboli/AI-Career-Compass-J-K#readme",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}